package regjava;

import java.io.*;
import java.lang.*;
import java.util.*;


public class Addnum {

	public static void main(String[] args) {
		int a,b;
		Scanner sc=new Scanner(System.in);
		
		System.out.println("Enter the value of a and b");
		a=sc.nextInt();
		b=sc.nextInt();
		
		System.out.println(a+b);
		

	}

}
